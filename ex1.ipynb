    "import torch\n",
    "\n",
    "def grad_example(x):\n",
    "    x.requires_grad_(True)\n",
    "    ex = torch.abs(x)\n",
    "    res = torch.max(ex)\n",
    "    res.backward()\n",
    "    print(x.grad)"
   ]
  },
